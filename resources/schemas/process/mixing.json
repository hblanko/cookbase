{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://www.landarltracker.com/schemas/process/mixing.json",
  "type": "object",
  "properties": {
    "process": {
      "const": "mixing"
    },
    "foodstuff": {
      "$ref": "../definitions.json#/foodstuff"
    },
    "instruments": {
      "$ref": "../definitions.json#/instruments"
    }
  },
  "required": [
    "process",
    "foodstuff",
    "instruments"
  ],
  "additionalProperties": false,
  "meta": {
    "conditions": {
      "necessaryInstruments": {
        "i1": ["family", "container"]
      }
    },
    "foodstuffKeywords": "foodstuff"
  }
}
