{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://www.landarltracker.com/schemas/process/storing.json",
  "type": "object",
  "properties": {
    "process": {
      "const": "storing"
    },
    "parameters": {
      "type": "object",
      "properties": {
        "timeMax": {
          "$ref": "../definitions.json#/quantities/time"
        },
        "temperatureDescriptive": {
          "$ref": "../definitions.json#/quantities/descriptive/temperature"
        }
      },
      "required": [
        "timeMax",
        "temperatureDescriptive"
      ],
      "additionalProperties": false
    },
    "foodstuff": {
      "$ref": "../definitions.json#/foodstuff"
    },
    "instruments": {
      "$ref": "../definitions.json#/instruments"
    }
  },
  "required": [
    "process",
    "foodstuff",
    "instruments"
  ],
  "additionalProperties": false,
  "meta": {
    "conditions": {
      "necessaryInstruments": {
        "i1": ["family", "container"]
      }
    },
    "foodstuffKeywords": "foodstuff"
  }
}
